{
  "name": "newcmms-server",
  "version": "1.0.0",
  "description": "Pet NewCMMS backend project",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "i:g:dev": "sudo npm i rimraf tslint nodemon tslint-config-airbnb -g -D",
    "r:g:dev": "sudo npm r rimraf tslint nodemon tslint-config-airbnb -g -D",
    "i:g:dev:win": "npm i rimraf tslint nodemon tslint-config-airbnb -g -D",
    "r:g:dev:win": "npm r rimraf tslint nodemon tslint-config-airbnb -g -D",
    "build": "rimraf ./dist && ttsc",
    "start": "node ./dist",
    "start:b": "npm run build && node ./dist",
    "start:brk": "node --inspect-brk ./dist",
    "start:b:brk": "npm run build && node --inspect-brk ./dist",
    "start:dev": "node --inspect=9229 ./dist",
    "start:b:dev": "npm run build && node --inspect=9229 ./dist",
    "watch": "nodemon -w src/ -w openapi/ -w config/ -e ts,yaml -x \"npm run start:b:dev\"",
    "db:p": "node dist/tools/db-populate",
    "db:populate": "node dist/tools/db-populate",
    "db:clear": "node dist/tools/db-clear",
    "keys": "node dist/tools/key-pairs",
    "keys:g": "node dist/tools/key-pairs g"
  },
  "engines": {
    "node": "^10.12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shevchenkobn/NewCMMS-server.git"
  },
  "author": "Bogdan Shevchenko",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/shevchenkobn/NewCMMS-server/issues"
  },
  "homepage": "https://github.com/shevchenkobn/NewCMMS-server#readme",
  "dependencies": {
    "@types/mime": "^2.0.1",
    "app-root-path": "^2.2.1",
    "async-sema": "^3.0.0",
    "bcrypt": "^3.0.5",
    "bluebird": "^3.5.4",
    "bluebird-global": "^1.0.1",
    "body-parser": "^1.18.3",
    "config": "^3.1.0",
    "cors": "^2.8.5",
    "decimal.js": "^10.1.1",
    "express": "^4.16.4",
    "express-openapi": "^4.5.0",
    "inversify": "^5.0.1",
    "json-schema-ref-parser": "^6.1.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.16.3",
    "linq": "^3.2.0",
    "mime": "^2.4.2",
    "pg": "^7.9.0",
    "randomatic": "^3.1.1",
    "reflect-metadata": "^0.1.13",
    "ts-optchain": "^0.1.6",
    "tslib": "^1.9.3",
    "yaml": "^1.5.0",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.26",
    "@types/bluebird-global": "^3.5.11",
    "@types/body-parser": "^1.17.0",
    "@types/config": "0.0.34",
    "@types/cors": "^2.8.4",
    "@types/decimal.js": "^7.4.0",
    "@types/express": "^4.16.1",
    "@types/json-schema": "^7.0.3",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/knex": "^0.16.1",
    "@types/node": "^11.13.6",
    "@types/pg": "^7.4.14",
    "@types/reflect-metadata": "^0.1.0",
    "@types/yaml": "^1.0.2",
    "@types/yargs": "^13.0.0",
    "ttypescript": "^1.5.6",
    "typescript": "^3.4.5"
  }
}
