{
  "name": "newcmms-server",
  "version": "1.0.0",
  "description": "Pet NewCMMS backend project",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "npm:g:dev": "sudo npm i rimraf tslint typescript nodemon tslint-config-airbnb -g -D",
    "npm:g:dev:win": "npm i rimraf tslint typescript nodemon tslint-config-airbnb -g -D",
    "build": "rimraf ./dist && tsc",
    "start": "node ./dist",
    "start:b": "npm run build && node ./dist",
    "start:brk": "node --inspect-brk ./dist",
    "start:b:brk": "npm run build && node --inspect-brk ./dist",
    "start:dev": "node --inspect=9229 ./dist",
    "start:b:dev": "npm run build && node --inspect=9229 ./dist",
    "watch": "nodemon -w src/ -w swagger/ -w config/ -e ts,yaml -x \"npm run start:b:dev\"",
    "db:drop": "node dist/console/drop-sql-tables",
    "db:add": "node dist/console/add-sql-tables",
    "genKeys": "node dist/console/generate-key-pair"
  },
  "engines": {
    "node": "^10.12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shevchenkobn/NewCMMS-server.git"
  },
  "author": "Bogdan Shevchenko",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/shevchenkobn/NewCMMS-server/issues"
  },
  "homepage": "https://github.com/shevchenkobn/NewCMMS-server#readme",
  "dependencies": {
    "app-root-path": "^2.2.1",
    "bcrypt": "^3.0.5",
    "body-parser": "^1.18.3",
    "config": "^3.1.0",
    "cors": "^2.8.5",
    "decimal.js": "^10.1.1",
    "express": "^4.16.4",
    "inversify": "^5.0.1",
    "js-yaml": "^3.13.1",
    "json-refs": "^3.0.13",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.16.3",
    "randomatic": "^3.1.1",
    "reflect-metadata": "^0.1.13",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.17.0",
    "@types/config": "0.0.34",
    "@types/cors": "^2.8.4",
    "@types/decimal.js": "^7.4.0",
    "@types/express": "^4.16.1",
    "@types/js-yaml": "^3.12.1",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/knex": "^0.16.1",
    "@types/node": "^11.13.2",
    "@types/reflect-metadata": "^0.1.0",
    "@types/yargs": "^13.0.0"
  }
}
